{
  "name": "multisynq-docs",
  "version": "1.0.0",
  "description": "Multisynq documentation powered by Mintlify",
  "scripts": {
    "docs:dev": "mintlify dev --port 3001",
    "docs:build": "mintlify build",
    "docs:deploy": "./build-docs.sh",
    "docs:serve": "mintlify dev",
    "lint": "echo 'No linting configured for MDX files yet'",
    "clean": "rm -rf .mintlify",
    "generate:llms": "node scripts/generate-llms-txt.js",
    "generate:llms:watch": "nodemon scripts/generate-llms-txt.js"
  },
  "keywords": [
    "multisynq",
    "documentation",
    "mintlify",
    "collaboration",
    "real-time"
  ],
  "author": "Multisynq",
  "license": "MIT",
  "dependencies": {
    "mintlify": "^4.0.0",
    "gray-matter": "^4.0.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/multisynq/multisynq"
  },
  "bugs": {
    "url": "https://github.com/multisynq/multisynq/issues"
  },
  "homepage": "https://docs.multisynq.io"
} 