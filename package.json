{
  "name": "multisynq-docs",
  "version": "1.0.0",
  "description": "Multisynq documentation powered by Mintlify",
  "scripts": {
    "docs:dev": "mint dev --port 3001",
    "docs:build": "mint build",
    "docs:deploy": "./build-docs.sh",
    "docs:serve": "mint dev",
    "docs:gen:client": "node scripts/generate-api-docs-v6.mjs --package client",
    "docs:gen:react": "node scripts/generate-api-docs-v5.mjs --package react",
    "docs:gen:react-together": "node scripts/generate-api-docs-v5.mjs --package react-together",
    "docs:gen:all": "npm run docs:gen:client && npm run docs:gen:react && npm run docs:gen:react-together && node scripts/sync-examples.mjs",
    "lint": "echo 'No linting configured for MDX files yet'",
    "clean": "rm -rf .mintlify"
  },
  "keywords": [
    "multisynq",
    "documentation",
    "mintlify",
    "collaboration",
    "real-time"
  ],
  "author": "Multisynq",
  "license": "MIT",
  "dependencies": {
    "glob": "^11.0.3",
    "jsdoc-to-markdown": "^8.0.0",
    "mintlify": "^4.0.0",
    "typedoc-json-parser": "^10.2.0",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "typedoc": "^0.25.13"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/multisynq/multisynq"
  },
  "bugs": {
    "url": "https://github.com/multisynq/multisynq/issues"
  },
  "homepage": "https://docs.multisynq.io"
}
