  "redirects": [
    { "source": "/api-reference/introduction", "destination": "/api-reference" },
    { "source": "/api-reference/model", "destination": "/api-reference#multisynq-model" },
    { "source": "/api-reference/view", "destination": "/api-reference#multisynq-view" },
    { "source": "/api-reference/session", "destination": "/api-reference#multisynq-session" },
    { "source": "/Model.html", "destination": "/api-reference#multisynq-model" },
    { "source": "/View.html", "destination": "/api-reference#multisynq-view" },
    { "source": "/Session.html", "destination": "/api-reference#multisynq-session" },
    { "source": "/Data.html", "destination": "/api-reference#multisynq-data" }
  ],
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      {
        "title": "Ask Gemini",
        "description": "Ask Google Gemini about the current page",
        "icon": "sparkle",
        "href": {
          "base": "https://gemini.google.com/app",
          "query": [
            {
              "key": "q",
              "value": "Ask a question about the following documentation page for Multisynq: https://docs.multisynq.io$path"
            }
          ]
        }
      },
      {
        "title": "Add Multisynq MCP",
        "description": "Install the Multisynq MCP server for this project",
        "icon": "stars",
        "href": {
            "base": "https://docs.multisynq.io/mcp-setup",
            "query": [
                {
                    "key": "cmd",
                    "value": "npx mint-mcp add multisynq.io"
                }
            ]
        }
      }
    ]
  }
}