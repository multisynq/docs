{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Multisynq Docs",
  "colors": {
    "primary": "#006EFF",
    "light": "#006EFF",
    "dark": "#006EFF"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "index",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Essentials",
            "pages": [
              "essentials/sync",
              "essentials/collaboration",
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/settings",
              "essentials/navigation",
              "essentials/reusable-snippets"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API Documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Sessions API",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Dashboard",
          "href": "https://multisynq.io",
          "icon": "chart-line"
        },
        {
          "anchor": "Community",
          "href": "https://discord.gg/multisynq",
          "icon": "discord"
        },
        {
          "anchor": "GitHub",
          "href": "https://github.com/multisynq",
          "icon": "github"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "https://discord.com/invite/multisynq"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Start building",
      "href": "https://multisynq.io"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/multisynq",
      "github": "https://github.com/multisynq",
      "linkedin": "https://www.linkedin.com/company/multisynq/"
    }
  }
}